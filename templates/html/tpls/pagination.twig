<nav>
  <ul class="pagination">
    <li class="page-item {{ prev_dis ? 'invisible' }}">
      <a class="page-link" href="{{ Previous }}" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">{{ strings.cms_pag_prev }}</span>
      </a>
    </li>
    {% for item in items %}
    <li class="page-item {{ item.iscur ? 'active' }}"><a class="page-link" href="{{ item.url }}">{{ item.page }}</a></li>
	{% endfor %}
    <li class="page-item {{ next_dis ? 'invisible' }}">
      <a class="page-link" href="{{ Next }}" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">{{ strings.cms_pag_next }}</span>
      </a>
    </li>
  </ul>
</nav>